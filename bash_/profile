export PATH=/usr/bin:/bin:/usr/sbin:/sbin:/usr/X11/bin:/usr/local/git/bin
export PATH=/usr/local/bin:/usr/local/share/python:/usr/local/share:/usr/local/sbin:/usr/local/lib:${PATH}
export PATH="$HOME/bin:$PATH"
export PATH=/usr/local/ActivePerl-5.14/bin:$PATH

if [ -e /usr/local/nolen_bin ]; then
    export PATH=${PATH}:/usr/local/nolen_bin
fi

if [ -e /usr/local/bin/brew ]; then
    export BYOBU_PREFIX=$(brew --prefix)
fi

export PYTHONSTARTUP=~/.pythonrc.py
