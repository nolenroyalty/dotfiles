# Macro to get prev command and get ready to add arg
"\e[[oo": "\C-p\C-a\ef "
# VT
"\e[1~":beginning-of-line
"\e[4~": end-of-line
# kvt
"\e[H":beginning-of-line
"\e[F":end-of-line
# rxvt and konsole
"\e[7~":beginning-of-line
"\e[8~":end-of-line
"\e[5~": history-search-backward
"\e[6~": history-search-forward
"\e[3~": delete-char
"\e[1;5C": forward-word
"\e[1;5D": backward-word
